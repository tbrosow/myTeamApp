<!-- Modal Update Game -->
<div class="modal fade" id="modalUpdateGame" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Update Game</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="alert alert-warning" role="alert">Update Game</div>
            <div class="modal-body" style="display: none">
                <label for="id" class="col-form-label">ID:</label>
                <input class="form-control" name="ug_id" id="ug_id">
            </div>
            <div class="modal-body" style="">
                <label for="ug_goalsscored" class="col-form-label">Goals Scored:</label>
                <input class="form-control" name="ug_goalsscored" id="ug_goalsscored">
            </div>
            <div class="modal-body" style="">
                <label for="ug_goalsconceded" class="col-form-label">Goals Conceded:</label>
                <input class="form-control" name="ug_goalsconceded" id="ug_goalsconceded">
            </div>
            <div class="modal-body" style="">
                <label for="ug_number" class="col-form-label">Number:</label>
                <input class="form-control" name="ug_number" id="ug_number">
            </div>
            <div class="modal-body" style="">
                <label for="ug_location" class="col-form-label">Ground / Oval:</label>
                <input class="form-control" name="ug_location" id="ug_location">
            </div>
            <div class="modal-body" style="">
                <label for="ug_homegame" class="col-form-label">Home / Away:</label>
                <input class="form-control" name="ug_homegame" id="ug_homegame">
            </div>
            <div class="modal-body" style="">
                <label for="ug_gameday" class="col-form-label">Game Day:</label>
                <input class="form-control" name="ug_gameday" id="ug_gameday">
            </div>
            <div class="modal-body" style="">
                <label for="ug_oponent" class="col-form-label">Oponent:</label>
                <input class="form-control" name="ug_oponent" id="ug_oponent">
            </div>
            <div class="modal-footer">
