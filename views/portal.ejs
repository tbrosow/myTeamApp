                        <%
                            for(var pl=1;pl<=20;pl++) {
                                var shirtTaken = false;
                                players.forEach(function(player){

                                    if (pl == player.shirt) {
                                        shirtTaken = true;
                                    }
                                });

                                if (!shirtTaken) {
                                    %> <li> <a href="#"><%= pl %> </a></li> <%
                                }
                            }
                        %>

                        <%  %>
                    </ul>
                </div><!-- /btn-group -->
                <input id="npshirt" type="text" class="form-control" aria-label="...">
